                <div class="col">
                    <div class="col-12 col-md-7 label">
                        <span class="col-form-label p-0" style="color: rgba(0, 0, 0, 0.54);">
                            Застройщик
                        </span>
                    </div>
                    <div class="col-12 col-md-4">
                        @if (Model.Project!.Participants.Any(p => p.ParticipantType == Diplom.Core.Data.Enums.ParticipantType.Type1))
                        {
                            var participant = Model.Project!.Participants.Single(p => p.ParticipantType == Diplom.Core.Data.Enums.ParticipantType.Type1);
                            MainController.GetParticipantName(participant);
                        }
                    </div>
                </div>

                <div class="col-2" style="text-align: right">
                    @if (Model.Project.Participants.Any(p => p.ParticipantType == Diplom.Core.Data.Enums.ParticipantType.Type1))
